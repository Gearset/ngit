<?xml version="1.0"?>
<!-- http://docs.nuget.org/docs/reference/nuspec-reference -->
<package >
	<metadata>
		<id>RedGate.ThirdParty.NGit</id>
		<version>$version$</version>
		<title>NGit</title>
		<authors>Mono</authors>
		<owners>Red Gate Software</owners>
		<requireLicenseAcceptance>false</requireLicenseAcceptance>
		<description>A backport of NGit library which is itself an automated port from JGit</description>
		<copyright>Copyright 2015</copyright>
		<projectUrl>https://github.com/red-gate/ngit</projectUrl>
		<dependencies>
			<dependency id="SharpZipLib" version="$csharpcodeVersionFromPackagesConfig$" />
		</dependencies>
	</metadata>
	<files>
		<file src="Nuspec\RedGate.ThirdParty.NGit.targets" target="build" />
		<file src="NGit\bin\$configuration$\netstandard2.0\NGit.*" target="lib\netstandard2.0" />
		<file src="NGit\bin\$configuration$\net461\NGit.*" target="lib\net461" />
		<file src="NGit.license.txt" target="App_Readme" />
		<file src="Sharpen\bin\$configuration$\netstandard2.0\Sharpen.*" target="lib\netstandard2.0" />
		<file src="Sharpen\bin\$configuration$\net461\Sharpen.*" target="lib\net461" />
	</files>
</package>
